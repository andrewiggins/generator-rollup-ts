{
    "compilerOptions": {
        "importHelpers": true,
        "inlineSources": true,
        "jsx": "react",
        "jsxFactory": "h",
        "lib": [ "dom", "es5" ],
        "module": "es6",
        "moduleResolution": "node",
        "outDir": "./lib",
        "sourceMap": true,
        "strict": true,
        "stripInternal": true,
        "target": "es5"
    },
    "include": [
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
